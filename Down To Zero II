#include <iostream>
#include <vector>
#include <queue>
#include <unordered_set>
using namespace std;

int minMoves(int n) {
    vector<int> dp(n+1, 1e9);
    dp[0] = 0; dp[1] = 1;
    for(int i = 2; i <= n; ++i) {
        dp[i] = dp[i-1] + 1;
        for(int j = 1; j*j <= i; ++j) {
            if(i % j == 0 && j != 1 && j != i) {
                int k = i / j;
                if(j < i)
                    dp[i] = min(dp[i], dp[j] + 1);
                if(k < i)
                    dp[i] = min(dp[i], dp[k] + 1);
            }
        }
    }
    return dp[n];
}

int main() {
    int q, n;
    cin >> q;
    while(q--) {
        cin >> n;
        cout << minMoves(n) << endl;
    }
    return 0;
}
