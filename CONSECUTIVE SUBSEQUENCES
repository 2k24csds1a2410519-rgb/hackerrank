t = int(input())
for _ in range(t):
    n, k = map(int, input().split())
    arr = list(map(int, input().split()))
    prefix = 0
    freq = [0] * k
    freq[0] = 1
    count = 0
    for num in arr:
        prefix = (prefix + num) % k
        count += freq[prefix]
        freq[prefix] += 1
    print(count
