#include <bits/stdc++.h>
using namespace std;
int main() {
    int T;
    cin >> T;
    while (T--) {
        int n, a, b;
        cin >> n >> a >> b;
        if (a > b) swap(a, b);
        set<int> s;
        for (int i = 0; i < n; i++) s.insert(a * (n - 1 - i) + b * i);
        for (auto x : s) cout << x << " ";
        cout << endl;
    }
}
