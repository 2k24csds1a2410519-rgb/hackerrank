#include <bits/stdc++.h>
using namespace std;

string gridSearch(vector<string> G, vector<string> P) {
    int R = G.size(), r = P.size(), c = P[0].size();
    for (int i = 0; i <= R - r; i++) {
        size_t pos = G[i].find(P[0]);
        while (pos != string::npos) {
            bool found = true;
            for (int j = 1; j < r; j++) {
                if (G[i + j].substr(pos, c) != P[j]) {
                    found = false;
                    break;
                }
            }
            if (found) return "YES";
            pos = G[i].find(P[0], pos + 1);
        }
    }
    return "NO";
}

int main() {
    int t;
    cin >> t;
    while (t--) {
        int R, C, r, c;
        cin >> R >> C;
        vector<string> G(R);
        for (int i = 0; i < R; i++) cin >> G[i];
        cin >> r >> c;
        vector<string> P(r);
        for (int i = 0; i < r; i++) cin >> P[i];
        cout << gridSearch(G, P) << endl;
    }
    return 0;
}
